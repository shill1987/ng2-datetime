    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.6.0",
    "bootstrap-timepicker": "^0.5.2",
